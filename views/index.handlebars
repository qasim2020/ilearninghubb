<script>
    document.addEventListener('DOMContentLoaded', function() {
        // Get all register buttons by class instead of a single button by ID
        const ticketBtns = document.querySelectorAll('.ticket-register-btn');
        const overlay = document.getElementById('ticketOverlay');
        const closeBtn = document.getElementById('closeTicketBtn');
        
        // Add click listener to each button
        ticketBtns.forEach(function(btn) {
            btn.addEventListener('click', function(e) {
                e.preventDefault();
                overlay.style.display = 'block';
                document.body.style.overflow = 'hidden'; // Prevent scrolling
            });
        });
        
        // Close overlay when close button is clicked
        closeBtn.addEventListener('click', function() {
            overlay.style.display = 'none';
            document.body.style.overflow = 'auto'; // Restore scrolling
        });
        
        // Close overlay when clicking outside the container
        overlay.addEventListener('click', function(e) {
            if (e.target === overlay) {
                overlay.style.display = 'none';
                document.body.style.overflow = 'auto'; // Restore scrolling
            }
        });
        
        // Listen for escape key
        document.addEventListener('keydown', function(e) {
            if (e.key === 'Escape' && overlay.style.display === 'block') {
                overlay.style.display = 'none';
                document.body.style.overflow = 'auto'; // Restore scrolling
            }
        });
    });
</script>



<!-- Custom Script to Disable Hero Slider Movement -->
<script>
document.addEventListener('DOMContentLoaded', function() {
  // Find the main slider Swiper instance
  const mainSlider = document.querySelector('.slider-one .main-slider').swiper;
  
  // If the Swiper instance exists
  if (mainSlider) {
    // Disable autoplay
    if (mainSlider.autoplay) {
      mainSlider.autoplay.stop();
    }
    
    // Disable swiping/sliding
    mainSlider.allowSlideNext = false;
    mainSlider.allowSlidePrev = false;
    mainSlider.allowTouchMove = false;
    
    // Destroy the pagination and navigation if they exist
    if (mainSlider.pagination) {
      mainSlider.pagination.destroy();
    }
    
    // Update the Swiper instance to apply changes
    mainSlider.update();
  }
});
</script>

<script>
    document.addEventListener('DOMContentLoaded', function() {
        // Add hover effect to navigation arrows
        const prevButtons = document.querySelectorAll('.swiper-button-prev');
        const nextButtons = document.querySelectorAll('.swiper-button-next');

        function addHoverEffect(button) {
            button.addEventListener('mouseenter', function() {
                this.style.background = '#F9A826'; // Warm yellow color
                this.style.borderColor = '#F9A826';
                this.querySelector('i').style.color = '#fff';
            });

            button.addEventListener('mouseleave', function() {
                this.style.background = 'transparent';
                this.style.borderColor = '#ccc';
                this.querySelector('i').style.color = '#333';
            });
        }

        prevButtons.forEach(addHoverEffect);
        nextButtons.forEach(addHoverEffect);
    });
</script>

